{
    "name": "improvframework/cryptography",
    "description": "A lightweight PHP library to handle basic cryptography operations",
    "type": "library",
    "keywords": [
        "keywords"
    ],
    "homepage": "https://github.com/improvframework/cryptography",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jim DeLois",
            "email": "jim.delois@improvframework.org",
            "homepage": "https://www.improvframework.org"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Improv\\Cryptography\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Improv\\Cryptography\\Test\\": "tests/lib"
        }
    },
    "require-dev": {
        "pdepend/pdepend": "^2.5",
        "phing/phing": "^2.16",
        "phpdocumentor/phpdocumentor": "^2.9",
        "phploc/phploc": "^3.0",
        "phpmd/phpmd": "^2.6",
        "phpmetrics/phpmetrics": "^1.10",
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^1.0",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.0"
    }
}
